{
  "name": "relay-web-app",
  "productName": "Forsta Messenger",
  "version": "0.63.5",
  "description:": "Secure communications inside and outside your organization",
  "author": "Forsta Labs <support@forsta.io>",
  "engines": {
    "node": "8",
    "npm": "5"
  },
  "cacheDirectories": [
    "components",
    "node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forstalabs/relay-web-app.git"
  },
  "scripts": {
    "heroku-postbuild": "make",
    "start": "node server/start.js",
    "startElectron": "make run-electron"
  },
  "bugs": {
    "url": "https://github.com/forstalabs/relay-web-app/issues"
  },
  "homepage": "https://github.com/forstalabs/relay-web-app",
  "main": "./electron/main.js",
  "build": {
    "mac": {
      "title": "Forsta Messenger for macOS",
      "icon": "./dist/static/images/app.icns",
      "background": "./dist/static/images/thething.png",
      "icon-size": 80,
      "contents": [
        {
          "x": 448,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 220,
          "type": "file",
          "path": "./dist/Forsta Messenger-darwin-x64/Forsta Messenger.app"
        }
      ]
    }
  },
  "dependencies": {
    "bower": "1.8.2",
    "emoji-datasource": "4.0.3",
    "emoji-datasource-google": "4.0.3",
    "emoji-js": "3.4.0",
    "express": "4.16.2",
    "grunt": "1.0.2",
    "grunt-cli": "1.2.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-sass": "1.0.0",
    "grunt-contrib-uglify-es": "github:gruntjs/grunt-contrib-uglify#ccb95a70cad6a4",
    "http-proxy": "1.16.2",
    "librelay-web": "github:ForstaLabs/librelay-web#a1c22a19b351aae",
    "libsignal-protocol": "github:ForstaLabs/libsignal-protocol-javascript#3a8629b6508",
    "morgan": "1.9.0"
  },
  "devDependencies": {
    "electron": "2.0.0-beta.2",
    "electron-builder": "^20.4.1",
    "electron-osx-sign": "^0.4.10",
    "electron-packager": "11.1.0",
    "eslint": "4.17.0",
    "grunt-contrib-watch": "1.0.0",
    "semantic-ui": "2.2.14"
  }
}
